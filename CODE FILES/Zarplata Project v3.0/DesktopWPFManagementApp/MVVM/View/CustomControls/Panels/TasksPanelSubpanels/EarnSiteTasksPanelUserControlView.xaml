        <Grid Name="AnimatedLoadImageGrid">
            <Grid.Style>
                <Style TargetType="Grid">
                    <Style.Setters>
                        <Setter Property="Visibility" Value="Hidden"/>
                        <Setter Property="Background" Value="Transparent"/>
                        <Setter Property="HorizontalAlignment" Value="Center"/>
                        <Setter Property="VerticalAlignment" Value="Center"/>
                    </Style.Setters>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding LoadingDataStatus, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Value="DataIsLoading">
                            <DataTrigger.Setters>
                                <Setter Property="Visibility" Value="Visible" />
                            </DataTrigger.Setters>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Grid.Style>

            <Grid>

                <Border
                    x:Name="LoadMainCenterBorderImg"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center" 
                    Width="125" 
                    Height="125"
                    KeyboardNavigation.TabNavigation="None"
                    Background="White"
                    CornerRadius="100"
                    Cursor="Hand">

                </Border>

                <Image 
                    x:Name="LoadMainImg"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center" 
                    Source="pack://application:,,,/Resources/Images/ruble2.png" 
                    Width="150" 
                    Height="150"
                    KeyboardNavigation.TabNavigation="None"
                    Cursor="Hand">

                </Image>

                <Image 
                    x:Name="LoadBorderBlurImg"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center" 
                    Source="pack://application:,,,/Resources/Images/ruble2_border.png" 
                    Width="150" 
                    Height="150"
                    KeyboardNavigation.TabNavigation="None">

                    <Image.Style>
                        <Style TargetType="Image">
                            <Style.Setters>
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <BlurEffect Radius="0" />
                                    </Setter.Value>
                                </Setter>
                            </Style.Setters>

                            <Style.Triggers>
                                <Trigger Property="IsEnabled" Value="True">
                                    <Trigger.EnterActions>
                                        <BeginStoryboard
                                            x:Name="RotateLoadBorderBlurImgBeginStoryboard">
                                            <Storyboard>
                                                <DoubleAnimation
                                                    Storyboard.TargetProperty="Effect.Radius"
                                                    From="0"
                                                    To="35"
                                                    Duration="0:0:0.800"
                                                    AutoReverse="True"
                                                    RepeatBehavior="Forever">
                                                </DoubleAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.EnterActions>
                                    <Trigger.ExitActions>
                                        <StopStoryboard BeginStoryboardName="RotateLoadBorderBlurImgBeginStoryboard" />
                                    </Trigger.ExitActions>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>

                </Image>

                <Image 
                    x:Name="LoadBorderImg"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center" 
                    Source="pack://application:,,,/Resources/Images/ruble2_border.png" 
                    Width="150" 
                    Height="150"
                    KeyboardNavigation.TabNavigation="None"
                    Cursor="Hand">

                    <Image.Style>
                        <Style TargetType="Image">
                            <Style.Setters>
                                <Setter Property="LayoutTransform">
                                    <Setter.Value>
                                        <RotateTransform Angle="0" CenterX="0" CenterY="0" />
                                    </Setter.Value>
                                </Setter>
                                <Setter Property="IsEnabled" Value="True" />
                            </Style.Setters>

                            <Style.Triggers>
                                <Trigger Property="IsEnabled" Value="True">
                                    <Trigger.EnterActions>
                                        <BeginStoryboard
                                            x:Name="RotateLoadBorderImgBeginStoryboard">
                                            <Storyboard>
                                                <DoubleAnimation
                                                    Storyboard.TargetProperty="(LayoutTransform).(RotateTransform.Angle)"
                                                    From="0"
                                                    To="360"
                                                    BeginTime="0:0:0.0"
                                                    Duration="0:0:10.000" 
                                                    AutoReverse="False"
                                                    RepeatBehavior="Forever">
                                                </DoubleAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.EnterActions>
                                    <Trigger.ExitActions>
                                        <StopStoryboard BeginStoryboardName="RotateLoadBorderImgBeginStoryboard" />
                                    </Trigger.ExitActions>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>

                </Image>

            </Grid>

        </Grid>






