                                VerticalAlignment="Center"
                                HorizontalAlignment="Left"
                                Margin="5 0 0 0">
                                </TextBlock>
                            </StackPanel>
                            <Button
                                    Width="Auto"
                                    Height="Auto"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    Margin="0 0 5 0"
                                    Cursor="Hand"
                                    Command="{Binding CloseDeleteTaskPanelCommand}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Setter Property="Template">
