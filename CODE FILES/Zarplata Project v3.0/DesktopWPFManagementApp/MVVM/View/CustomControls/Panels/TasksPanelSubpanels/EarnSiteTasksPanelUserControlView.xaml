<UserControl x:Class="DesktopWPFManagementApp.MVVM.View.CustomControls.Panels.TasksPanelSubpanels.EarnSiteTasksPanelUserControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopWPFManagementApp.MVVM.View.CustomControls.Panels.TasksPanelSubpanels"
             xmlns:usercontrolsElements="clr-namespace:DesktopWPFManagementApp.MVVM.View.CustomControls.Elements"
             xmlns:fontawesome="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
             xmlns:panels="clr-namespace:DesktopWPFManagementApp.MVVM.ViewModel.CustomControls.TasksPanelSubpanels"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="1200"
             MinHeight="500" MinWidth="1200"
             Name="root">

    <!--<UserControl.DataContext>
        <panels:EarnSiteTasksPanelUserControlViewModel/>
    </UserControl.DataContext>-->

    <Grid>

        <Border Name="MainContentBorder">
            <Border.Style>
                <Style TargetType="Border">
                    <Style.Setters>
                        <Setter Property="Effect">
                            <Setter.Value>
                                <BlurEffect Radius="0" />
                            </Setter.Value>
                        </Setter>
                    </Style.Setters>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding LoadingDataStatus, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Value="DataIsLoading">
                            <DataTrigger.Setters>
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <BlurEffect Radius="30"/>
                                    </Setter.Value>
                                </Setter>
                            </DataTrigger.Setters>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding IsFilterPopupPanelOpen, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Value="True">
                            <DataTrigger.Setters>
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <BlurEffect Radius="30"/>
                                    </Setter.Value>
                                </Setter>
                            </DataTrigger.Setters>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding IsTaskInfoPopupPanelOpen, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Value="True">
                            <DataTrigger.Setters>
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <BlurEffect Radius="30"/>
                                    </Setter.Value>
                                </Setter>
                            </DataTrigger.Setters>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding IsDeleteTaskPopupPanelOpen, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Value="True">
                            <DataTrigger.Setters>
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <BlurEffect Radius="30"/>
                                    </Setter.Value>
                                </Setter>
                            </DataTrigger.Setters>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Border.Style>

            <Grid>

                <Grid.RowDefinitions>
                    <RowDefinition Height="60" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="100" />
                </Grid.RowDefinitions>

                <Border
            Grid.Row="0"
            Background="{DynamicResource SecondaryBackColor}">

                    <DockPanel
                Background="Transparent">

                        <StackPanel
                    Orientation="Horizontal"
                    DockPanel.Dock="Left"
                    FlowDirection="LeftToRight">

                            <TextBlock
                        Text="Задания"
                        FontSize="16"
                        FontFamily="Arial"
                        FontWeight="SemiBold"
                        Foreground="White"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Margin="20 0 0 0">
                            </TextBlock>

                            <TextBlock
                        x:Name="MarkedTasksCountLeftBracket"
                        Text="("
                        FontSize="16"
                        FontFamily="Arial"
                        FontWeight="Light"
                        Foreground="White"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Margin="5 0 0 0">

                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=MarkedTasksCount}" Value="0">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>

                            </TextBlock>

                            <TextBlock
                        Text="{Binding MarkedTasksCount, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                        FontSize="16"
                        FontFamily="Arial"
                        FontWeight="Light"
                        Foreground="White"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Margin="0 0 0 0">

                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=MarkedTasksCount}" Value="0">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>

                            <TextBlock
                        x:Name="MarkedTasksCountRightBracket"
                        Text=")"
                        FontSize="16"
                        FontFamily="Arial"
                        FontWeight="Light"
                        Foreground="White"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Margin="0 0 0 0">

                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=MarkedTasksCount}" Value="0">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>

                        </StackPanel>

                        <StackPanel
                            Orientation="Horizontal"
                            FlowDirection="LeftToRight"
                            DockPanel.Dock="Left">

                            <Button
                                Width="145"
                                Height="40"
                                Margin="10 0 0 0"
                                FontSize="11"
                                FontFamily="Arial"
                                FontWeight="Medium"
                                Background="{DynamicResource DefaultPrimaryButtonColor}"
                                Command="{Binding CreateAndStartTasksCommand}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Setters>
                                            <Setter Property="Template" Value="{StaticResource DefaultButtonTemplate}"/>
                                            <Setter Property="Background" Value="{DynamicResource PrimaryBackColor}" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Content">
                                                <Setter.Value>
                                                    <Border>
                                                        <StackPanel 
                                                            Orientation="Horizontal"
                                                            FlowDirection="LeftToRight">

                                                            <TextBlock
                                                                Text="Запустить заработок">

                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray" />
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>

                                                            </TextBlock>

                                                            <fontawesome:IconImage 
                                                                Icon="Coins"
                                                                Cursor="Hand"
                                                                Margin="5 0 0 0">

                                                                <fontawesome:IconImage.Style>
                                                                    <Style TargetType="fontawesome:IconImage">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray"/>
                                                                            <Setter Property="Height" Value="10"/>
                                                                            <Setter Property="Width" Value="10"/>
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </fontawesome:IconImage.Style>

                                                            </fontawesome:IconImage>

                                                        </StackPanel>
                                                    </Border>
                                                </Setter.Value>
                                            </Setter>
                                        </Style.Setters>
                                    </Style>
                                </Button.Style>

                            </Button>

                            <Button
                                Width="145"
                                Height="40"
                                Margin="10 0 0 0"
                                FontSize="11"
                                FontFamily="Arial"
                                FontWeight="Medium"
                                Background="{DynamicResource DefaultPrimaryButtonColor}"
                                Command="{Binding CreateAndStartAutoregTasksCommand}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Setters>
                                            <Setter Property="Template" Value="{StaticResource DefaultButtonTemplate}"/>
                                            <Setter Property="Background" Value="{DynamicResource PrimaryBackColor}" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Content">
                                                <Setter.Value>
                                                    <Border>
                                                        <StackPanel 
                                                            Orientation="Horizontal"
                                                            FlowDirection="LeftToRight">

                                                            <TextBlock
                                                                Text="Запустить авторег">

                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray" />
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>

                                                            </TextBlock>

                                                            <fontawesome:IconImage 
                                                                Icon="UserAlt"
                                                                Cursor="Hand"
                                                                Margin="5 0 0 0">

                                                                <fontawesome:IconImage.Style>
                                                                    <Style TargetType="fontawesome:IconImage">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray"/>
                                                                            <Setter Property="Height" Value="10"/>
                                                                            <Setter Property="Width" Value="10"/>
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </fontawesome:IconImage.Style>

                                                            </fontawesome:IconImage>

                                                        </StackPanel>
                                                    </Border>
                                                </Setter.Value>
                                            </Setter>
                                        </Style.Setters>
                                    </Style>
                                </Button.Style>

                            </Button>

                            <Button
                                Width="145"
                                Height="40"
                                Margin="10 0 0 0"
                                FontSize="11"
                                FontFamily="Arial"
                                FontWeight="Medium"
                                Background="{DynamicResource DefaultPrimaryButtonColor}"
                                Command="{Binding CreateAndStartWithdrawalOfMoneyTasksCommand}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Setters>
                                            <Setter Property="Template" Value="{StaticResource DefaultButtonTemplate}"/>
                                            <Setter Property="Background" Value="{DynamicResource PrimaryBackColor}" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Content">
                                                <Setter.Value>
                                                    <Border>
                                                        <StackPanel 
                                                            Orientation="Horizontal"
                                                            FlowDirection="LeftToRight">

                                                            <TextBlock
                                                                Text="Запустить вывод">

                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray" />
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>

                                                            </TextBlock>

                                                            <fontawesome:IconImage 
                                                                Icon="MoneyBillTransfer"
                                                                Cursor="Hand"
                                                                Margin="5 0 0 0">

                                                                <fontawesome:IconImage.Style>
                                                                    <Style TargetType="fontawesome:IconImage">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray"/>
                                                                            <Setter Property="Height" Value="12"/>
                                                                            <Setter Property="Width" Value="12"/>
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </fontawesome:IconImage.Style>

                                                            </fontawesome:IconImage>

                                                        </StackPanel>
                                                    </Border>
                                                </Setter.Value>
                                            </Setter>
                                        </Style.Setters>
                                    </Style>
                                </Button.Style>

                            </Button>

                        </StackPanel>

                        <StackPanel
                    Orientation="Horizontal"
                    DockPanel.Dock="Right"
                    FlowDirection="RightToLeft">

                            <Button
                                Width="85"
                                Height="40"
                                Margin="20 0 0 0"
                                FontSize="11"
                                FontFamily="Arial"
                                FontWeight="Medium"
                                Background="{DynamicResource DefaultPrimaryButtonColor}"
                                Command="{Binding SearchKeywordButtonIsPressed}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Setters>
                                            <Setter Property="Template" Value="{StaticResource DefaultButtonTemplate}"/>
                                            <Setter Property="Background" Value="{DynamicResource PrimaryBackColor}" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Content">
                                                <Setter.Value>
                                                    <Border>
                                                        <StackPanel 
                                                            Orientation="Horizontal"
                                                            FlowDirection="LeftToRight">

                                                            <TextBlock
                                                                Text="Поиск">

                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray" />
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>

                                                            </TextBlock>

                                                            <fontawesome:IconImage 
                                                                Icon="Search"
                                                                Cursor="Hand"
                                                                Margin="5 0 0 0">

                                                                <fontawesome:IconImage.Style>
                                                                    <Style TargetType="fontawesome:IconImage">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray"/>
                                                                            <Setter Property="Height" Value="10"/>
                                                                            <Setter Property="Width" Value="10"/>
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </fontawesome:IconImage.Style>

                                                            </fontawesome:IconImage>

                                                        </StackPanel>
                                                    </Border>
                                                </Setter.Value>
                                            </Setter>
                                        </Style.Setters>
                                    </Style>
                                </Button.Style>

                            </Button>

                            <usercontrolsElements:SearchEarnSiteTasksTextBoxUserControlView
                            FlowDirection="LeftToRight"
                            x:Name="TasksSearch"
                            Height="40" 
                            Width="auto" 
                            Placeholder="Название.."
                            FontfamilyCustom="Arial"
                            Margin="10 0 0 0"
                            ComboboxItemSourceList="{Binding SearchUserControlListCombobox, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"
                            ComboboxSelectedItem="{Binding SelectedItemSearchUserControlCombobox, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                            Text="{Binding SearchUserControlKeywordText, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                            IncorrectInputData="{Binding IncorrectInputData, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}">

                            </usercontrolsElements:SearchEarnSiteTasksTextBoxUserControlView>

                            <Button
                                Name="OpenFilterPanelButton"
                                Width="85"
                                Height="40"
                                Margin="10 0 0 0"
                                FontSize="11"
                                FontFamily="Arial"
                                FontWeight="Medium"
                                Command="{Binding OpenTaskCollectionFilterPanelCommand}">

                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Setters>
                                            <Setter Property="Template" Value="{StaticResource DefaultButtonTemplate}"/>
                                            <Setter Property="Background" Value="{DynamicResource PrimaryBackColor}" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Content">
                                                <Setter.Value>
                                                    <Border>
                                                        <StackPanel 
                                                            Orientation="Horizontal"
                                                            FlowDirection="LeftToRight">

                                                            <TextBlock
                                                                Text="Фильтер">

                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray" />
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>

                                                            </TextBlock>

                                                            <fontawesome:IconImage 
                                                                Icon="Filter"
                                                                Cursor="Hand"
                                                                Margin="5 0 0 0">

                                                                <fontawesome:IconImage.Style>
                                                                    <Style TargetType="fontawesome:IconImage">
                                                                        <Style.Setters>
                                                                            <Setter Property="Foreground" Value="DarkGray"/>
                                                                            <Setter Property="Height" Value="10"/>
                                                                            <Setter Property="Width" Value="10"/>
                                                                        </Style.Setters>

                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                                                                <DataTrigger.Setters>
                                                                                    <Setter Property="Foreground" Value="White"/>
                                                                                </DataTrigger.Setters>
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </fontawesome:IconImage.Style>

                                                            </fontawesome:IconImage>

                                                        </StackPanel>
                                                    </Border>
                                                </Setter.Value>
                                            </Setter>
                                        </Style.Setters>
                                    </Style>
                                </Button.Style>

                            </Button>

                            <ComboBox 
                        ItemContainerStyle="{DynamicResource ComboBoxItemContainerStyleOne}" 
                        Style="{DynamicResource ComboBoxStyleOne}"
                        Height="40"
                        Width="Auto"
                        ItemsSource="{Binding Path=SearchSortCountListCombobox}"
                        SelectedItem="{Binding Path=SelectedSearchSortCountCombobox}"
                        DisplayMemberPath="Key"
                        Margin="10 0 0 0"
                        FlowDirection="LeftToRight">

                            </ComboBox>

                        </StackPanel>

                    </DockPanel>

                </Border>

                <Border
            Grid.Row="1">

                    <DataGrid
                RowStyle="{DynamicResource GridRowStyleOne}"
                CellStyle="{DynamicResource GridCellStyleOne}" 
                Style="{DynamicResource GridPanelStyle}"
                ItemsSource="{Binding TasksCollection, UpdateSourceTrigger=PropertyChanged}"
                Margin="15 15 15 0"
                AlternationCount="999999999"
                Height="auto"
                Width="auto">

                        <DataGrid.InputBindings>
                            <KeyBinding
                        Modifiers="Ctrl"
                        Key="C"
                        Command="{Binding Path=DataContext.CopyRowValueCommand, ElementName=root}"
                        CommandParameter="{Binding Path=SelectedItem, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=DataGrid}}"/>
                            <MouseBinding MouseAction="RightClick" Command="{Binding DataGridContextMenuPreparationBeforeOpenCommand}" />
                        </DataGrid.InputBindings>

                        <DataGrid.ContextMenu>
                            <ContextMenu>
                                <MenuItem Command="{Binding OpenDeleteFewTasksPanelCommand}">
                                    <MenuItem.Style>
                                        <Style TargetType="MenuItem">
                                            <Style.Setters>
                                                <Setter Property="Foreground" Value="Black"/>
                                                <Setter Property="Icon">
                                                    <Setter.Value>
                                                        <Image 
                                                            Source="{DynamicResource DeleteTrashIconImg}"
                                                            />
                                                    </Setter.Value>
                                                </Setter>
                                                <Setter Property="Header">
                                                    <Setter.Value>
                                                        <StackPanel Orientation="Horizontal">
                                                            <TextBlock
                                                                Text="Удалить "
                                                                />
                                                            <TextBlock
                                                                Text="{Binding MarkedTasksCount}"
                                                                />
                                                            <TextBlock
                                                                Text=" выбранных заданий"
                                                                />
                                                        </StackPanel>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style.Setters>
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding DeleteTaskContextMenuActionCanExecute}" Value="false">
                                                    <Setter Property="Foreground" Value="DarkGray"/>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </MenuItem.Style>

                                </MenuItem>
                            </ContextMenu>
                        </DataGrid.ContextMenu>

                        <DataGrid.ColumnHeaderStyle>
                            <Style TargetType="DataGridColumnHeader">
                                <Style.Setters>
                                    <Setter Property="Background" Value="Transparent"/>
                                    <Setter Property="BorderThickness" Value="0"/>
                                    <Setter Property="BorderBrush" Value="Transparent"/>
                                </Style.Setters>
                            </Style>
                        </DataGrid.ColumnHeaderStyle>
                        <DataGrid.RowHeaderStyle>
                            <Style TargetType="DataGridRowHeader">
                                <Style.Setters>
                                    <Setter Property="Background" Value="Transparent"/>
                                    <Setter Property="BorderThickness" Value="0"/>
                                    <Setter Property="BorderBrush" Value="Transparent"/>
                                </Style.Setters>
                            </Style>
                        </DataGrid.RowHeaderStyle>

                        <DataGrid.Columns>
                            <DataGridTemplateColumn
                        CanUserResize="False"
                        Width="auto"
                        HeaderStyle="{StaticResource GridColumnHeaderStyleOneFirstColumn}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <CheckBox 
                                    Style="{DynamicResource CheckBoxStyleOne}"
                                    Command="{Binding Path=DataContext.MarkAllElementsCommand, ElementName=root}"
                                    CommandParameter="{Binding Path=IsChecked, RelativeSource={RelativeSource Mode=Self}}">

                                        </CheckBox>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>

                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <CheckBox
                                    Style="{DynamicResource CheckBoxStyleOne}"
                                    IsChecked="{Binding Path=IsMarked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                    Command="{Binding Path=DataContext.MarkOneElementCommand, ElementName=root}"
                                    CommandParameter="{Binding Path=IsChecked, RelativeSource={RelativeSource Mode=Self}}"/>

                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn
                                Header="Id"
                                Width="200*"
                                HeaderStyle="{StaticResource GridColumnHeaderStyleOne}"
                                SortMemberPath="Id">


                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition MinWidth="30" Width="*"/>
                                                <ColumnDefinition Width="10"/>
                                                <ColumnDefinition Width="30"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel
                                                Grid.Column="0"
                                                Orientation="Horizontal">

                                                <Image 
                                                    Source="{StaticResource TaskIdIconImg}"
                                                    HorizontalAlignment="Left" 
                                                    VerticalAlignment="Center"
                                                    Width="25" 
                                                    Height="25"
                                                    Cursor="Hand">

                                                </Image>

                                                <TextBlock
                                                    Margin="5 0 0 0"
                                                    HorizontalAlignment="Left" 
                                                    VerticalAlignment="Center"
                                                    Text="{Binding Id}"/>

                                            </StackPanel>

                                            <Border
                                                Grid.Column="2"
                                                Height="30"
                                                Width="30"
                                                Margin="0 0 0 0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Background="{DynamicResource BotsCopyCellIconButtonColor}"
                                                Opacity="1"
                                                CornerRadius="5">

                                                <Button
                                                    Width="15"
                                                    Height="15"
                                                    Margin="0 0 0 0"
                                                    Style="{DynamicResource CopyIconButtonStyle}"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{Binding Path=DataContext.CopyCellValueCommand, ElementName=root}"
                                                    CommandParameter="{Binding Path=Id}">

                                                    <fontawesome:IconImage 
                                                        Icon="Copy"
                                                        Cursor="Hand">

                                                        <fontawesome:IconImage.Style>
                                                            <Style TargetType="fontawesome:IconImage">
                                                                <Style.Setters>
                                                                    <Setter Property="Foreground" Value="DarkGray"/>
                                                                </Style.Setters>
                                                            </Style>
